<div id="identity-box">
  <ul class="dropdown-menu" aria-labelledby="dropdownMenu4">
    <li><a href="#">Regular link</a></li>
    <li class="disabled"><a href="#">Disabled link</a></li>
    <li><a href="#">Another link</a></li>
  </ul>
</div>

<div id="repositories" class="sidebar-box">
  <div class="box-header">
    <h3>Repositories</h3>
  </div>
  <div class="box-content">
    <div class="input-group">
      <input type="text" class="form-control" aria-label="...">
      <% for i in 1..10 %>
        <div class="repo-link">
          <span class="glyphicon glyphicon-lock"></span><a><%= Faker::GameOfThrones.city.split(' ').join('-') %>/<%= Faker::GameOfThrones.city.split(' ').join('-') %></a>
        </div>
      <% end %>
    </div>
  </div>
</div>

<div id="teams" class="sidebar-box">
  <div class="box-header">
    <h3>Repositories</h3>
  </div>
  <div class="box-content">
    <div class="input-group">
      <input type="text" class="form-control" aria-label="...">
    </div>
    <% for i in 1..3 %>
      <div class="repo-link">
        <span class="glyphicon glyphicon-tower"></span><a><%= Faker::GameOfThrones.house %></a>
      </div>
    <% end %>
  </div>
</div>